  var strSvcUrl = "selectEmpListAll.do" ; //server에서 호출할 .do 의 url 
  var inData    = "" ; // server단의로 넘길 dataset 
  var outData   = "ds_emp=out_emp" ; //server에서 결과로 넘어오는 명칭 
  var callBackFnc = "fnCallback";   //콜백함수명 지정
  var isAsync     = true;           //동기 OR 비동기 설정 
	
